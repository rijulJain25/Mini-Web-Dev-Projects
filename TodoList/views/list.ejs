
 <%- include("header"); -%>
    <div class="box" id="heading">
      <h1><%= listTitle %></h1>
    </div>

    <div class="box">
      <% for(var i = 0; i< newListItems.length; i++) {%>
           <div class="item">
             <input type="checkbox" >
             <p><%= newListItems[i] %></p>
           </div>
      <% } %>

      <form class ="item" action="/" method="post">
        <input type="text" name="newItem" placeholder="New Item" autocomplete="off">
        <button type="submit" name="list" value=<%=listTitle %>>+</button>
      </form>
      <form class="item" action="/" method="delete">
        <input type="text" name="delItem" placeholder="Delete Item" autocomplete="off">
        <button type="submit" name="button2">-</button>
      </form>
    </div>

 <%- include("footer"); -%>
